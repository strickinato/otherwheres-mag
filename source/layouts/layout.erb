<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">

    <meta property="og:url"         content="http://otherwheresmag.com" />
    <meta property="og:title"       content="Otherwheres Magazine" />
    <meta property="og:image"       content="http://otherwheresmag.com/assets/branding/site-pic.jpg" />
    <meta property="og:description" content="{ Mostly True } Stories" />
    <meta property="og:type"        content="website" />

    <meta name="twitter:card"       content="summary" />
    <meta name="twitter:site"       content="@otherwheresmag" />
    <meta name="twitter:creator"    content="@jbienkahn" />

    <link rel="icon" type="image/png" href="assets/branding/favicon-16x16.png" sizes="16x16">
    <link rel="icon" type="image/png" href="assets/branding/favicon-32x32.png" sizes="32x32">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Otherwheres" %></title>

    <link href='//fonts.googleapis.com/css?family=Lato:300,400' rel='stylesheet' type='text/css'>

    <%= stylesheet_link_tag :site %>
    <%= javascript_include_tag :all %>
  </head>

